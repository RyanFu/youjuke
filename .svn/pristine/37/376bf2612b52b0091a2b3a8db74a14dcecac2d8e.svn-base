public class ChanceStep {
    private String step;//1：立即邀请  2：购买虚拟商品 3：分享报价器
    private String msg;


    public ChanceStep(String step, String msg) {
        this.step = step;
        this.msg = msg;
    }

    public ChanceStep() {
    }

    public String getStep() {
        return step;
    }

    public void setStep(String step) {
        this.step = step;
    }

    public String getMsg() {
        return msg;
    }

    public void setMsg(String msg) {
        this.msg = msg;
    }
}
